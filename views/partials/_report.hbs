
  <div class="col s12 m6">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title">Daily Report</span>
        <p id="date">Created on {{this.fancyTime}}</p>
          <p> You made ${{this.takeHome}} this day.</p>
      </div>
      <div class="card-action">

        <div class="row mb-0">

          <div class="col">
            <a class="btn waves-effect light-blue lighten-1" href="/report/detailed/{{this._id}}">Details
              <i class="material-icons right">more</i>
            </a>
          </div>
          <div class="col">
            <a class="btn waves-effect amber darken-1" href="/report/edit/{{this._id}}">Edit
              <i class="material-icons right">edit</i>
            </a>
          </div>
          <div class="col">
            <form action="/report/delete/{{this._id}}" method="POST">
              <button class="btn waves-effect waves-light red darken-1">Delete
                <i class="material-icons right">delete_forever</i>
              </button>
            </form>
          </div>
        </div>

      </div>
    </div>
  </div>
